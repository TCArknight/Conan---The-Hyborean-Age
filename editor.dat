<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">


  <!-- Attribute editthing -->
  <editthing
    compset="Attribute"
    name="Attribute"
    prefix="attr"
    description="Attributes are incredibly simple. They have a minimum and maximum value.{br}They also have an optional short name and/or abbreviation."
    summary="Defines an Attribute upon which starting CPs can be spent by a character.">
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the attribute (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the attribute (default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the attribute via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
    </editthing>


  <!-- Skill editthing -->
  <editthing
    compset="Skill"
    name="Skill"
    prefix="sk"
    description="As basic traits, Skills have a minimum and maximum value, as well as{br}an optional short name and/or abbreviation."
    summary="Defines a Skill upon which starting CPs can be spent by a character.">
    <inputthing
      name="Skill Category"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagpick group="SkillCat" tag="?"/>
      </inputthing>
    <inputthing
      name="XP Cost"
      helptext="Specify the XP Cost for the skill (default is 1).">
      <it_field field="sklXPCost"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify the abbreviation for the skill (default is 1).">
      <it_field field="abilAbbrev"/>
      </inputthing>

	<inputthing
	  name="Sorcery">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Inherent Sorcery Skill?"
      helptext="Specify whether this skill is considered an Inherent skill.">
      <it_tagcheck group="SkillType" tag="Inherent"/>
      </inputthing>
    <inputthing
      name="Belongs to Sorcery"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagpick group="Sorcery" tag="?"/>
      </inputthing>
    </editthing>


  <!-- Ability editthing -->
  <editthing
    compset="Ability"
    name="Abilities"
    prefix="ab"
    description="As basic traits, special abilities have various characteristics that are not currently used.{br}They also have an optional custom name for display on the Special tab."
    summary="Defines a Special Ability upon which starting CPs can be spent by a character.">
    <inputthing
      name="Name for Special Tab"
      helptext="Specify an optional custom name to be used on the Special tab.">
      <it_field field="spcName"/>
      </inputthing>
    <inputthing
      name="Hide from User?"
      helptext="Specify whether this special ability is hidden from direct user selection (e.g. Flight is only conferred by the Avian race).">
      <it_tagcheck group="Hide" tag="Ability"/>
      </inputthing>
    <inputthing
      name="Activated by User?"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagcheck group="User" tag="Activation"/>
      </inputthing>
    <inputthing
      name="Creation Only?"
      helptext="Specify whether this special ability can only be selected for a character during creation.">
      <it_tagcheck group="User" tag="CreateOnly"/>
      </inputthing>
<!-- These facets of special abilities are not currently used for anything so they are commented out.
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the special ability (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the skill(default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the skill via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
-->
    </editthing>


  <!-- Origin editthing -->
  <editthing
    compset="Origin"
    name="Origin"
    prefix="ori"
    description="Origins have ..."
    summary="Defines a Origin that can be selected by a character.">
    <inputthing
      name="Bonus Text"
      helptext="Specify the standard cost of the item, with a value of 0 meaning it is free.">
      <it_field field="oriBonusText"/>
      </inputthing>
    <inputthing
      name="Life Points"
      helptext="Specify the standard cost of the item, with a value of 0 meaning it is free.">
      <it_field field="oriLifePoints"/>
      </inputthing>
    <inputthing
      name="Sorcery Text"
      helptext="Specify the standard cost of the item, with a value of 0 meaning it is free.">
      <it_field field="oriSorcText"/>
      </inputthing>
	<inputthing
        name="Origin Abilities"
        helptext="Select any abilities for the origin.">
	  <it_bootlist compset="OriginAbility" />
      </inputthing>
	<inputthing
        name="Skills"
        helptext="Select any Skills for the origin.">
	  <it_bootlist compset="Skill" />
      </inputthing>
    <inputthing
      name="Sorcery Allowed"
      helptext="Specify any allowed sorcery.">
      <it_taglist group="SorceryAllowed" tag="?" />
      </inputthing>
    </editthing>


